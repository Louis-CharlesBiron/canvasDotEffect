{
    "name": "cdejs",
    "version": "1.3.0",
    "description": "CanvasDotEffect is a lightweight JS library that helps create customizable and interactive dot-based effects using the Canvas API",
    "author": {
        "name": "Louis-Charles Biron",
        "email": "louirlesbiron6@gmail.com",
        "url": "https://github.com/Louis-CharlesBiron"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Louis-CharlesBiron/canvasDotEffect"
    },
    "type": "module",
    "main": "canvasDotEffect.min.js",
    "module": "cde.min.js",
    "browser": "canvasDotEffect.min.js",
    "types": "dts/cde.d.ts",
    "files": [
        "cde.min.js",
        "canvasDotEffect.min.js",
        "bins/createBrowserProjectTemplate.min.js",
        "bins/createDefaultReactComponent.min.js",
        "bins/createProjectTemplate.min.js",
        "bins/getSignature.min.js",
        "bins/openDocumentation.min.js",
        "bins/global.min.js",
        "readme.md",
        "LICENSE",
        "dts/canvasDotEffect.d.ts",
        "dts/cde.d.ts"
    ],
    "exports": {
        ".": {
            "import": "./cde.min.js",
            "require": "./canvasDotEffect.min.js",
            "types": "./dts/cde.d.ts"
        },
        "./canvasDotEffect": {
            "require": "./canvasDotEffect.min.js",
            "types": "./dts/canvasDotEffect.d.ts"
        }
    },
    "bin": {
        "cdejs": "./bins/global.min.js", 
        "cdejs-template": "./bins/createProjectTemplate.min.js",
        "cdejs-t": "./bins/createProjectTemplate.min.js",
        "cdejs-browser-template": "./bins/createBrowserProjectTemplate.min.js",
        "cdejs-bt": "./bins/createBrowserProjectTemplate.min.js",
        "cdejs-react": "./bins/createDefaultReactComponent.min.js",
        "cdejs-doc": "./bins/openDocumentation.min.js",
        "cdejs-documentation": "./bins/openDocumentation.min.js",
        "cdejs-signature": "./bins/getSignature.min.js",
        "cdejs-sig": "./bins/getSignature.min.js",
        "cdejs-s": "./bins/getSignature.min.js"
    },
